{
  "builds": [
    {
      "src": "/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/privacy.html",
      "dest": "/privacy.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    }
  ]
}

